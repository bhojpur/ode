<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
  
<suite name="OdeJava.unit" time-out="5000">
  
  <test name="OdeJava.unit.active">
    <groups>
      <run>
        <exclude name="broken"/>
        <exclude name="ignore"/>
        <exclude name="integration"/>
      </run>
    </groups>
    <packages>
      <package name="utests.*"/>
    </packages>
  </test>

</suite>