<?xml version="1.0" encoding="utf-8"?>

<FindBugsFilter>

  <!--
    The code generated by slice2java is out of our control, and
    should be filtered from bug detection.
  -->
  <Match> <Package name="ode.api"/> </Match>
  <Match> <Package name="ode.grid"/> </Match>
  <Match> <Package name="ode.model"/> </Match>
  <Match> <Package name="ode.sys"/> </Match>
  
  <!--
    Filter out any Unread Field Bug if the field in the class is called log
  -->
  <Match>
    <Bug pattern="URF_UNREAD_FIELD" />
    <Field name="log" />
  </Match>
</FindBugsFilter>
